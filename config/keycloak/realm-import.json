{
  "realm": "microservices",
  "displayName": "Microservices Realm",
  "displayNameHtml": "<strong>Microservices</strong> Authentication",
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": true,
  "registrationEmailAsUsername": false,
  "rememberMe": true,
  "verifyEmail": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 30,
  "defaultRoles": ["offline_access", "uma_authorization", "user"],
  "requiredCredentials": ["password"],
  "passwordPolicy": "length(8) and digits(1) and lowerCase(1) and upperCase(1)",
  "otpPolicyType": "totp",
  "otpPolicyAlgorithm": "HmacSHA1",
  "otpPolicyInitialCounter": 0,
  "otpPolicyDigits": 6,
  "otpPolicyLookAheadWindow": 1,
  "otpPolicyPeriod": 30,
  "clients": [
    {
      "clientId": "adminer",
      "name": "Adminer Database Tool",
      "description": "Database administration interface",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "adminer-secret",
      "redirectUris": [
        "https://adminer.test/*",
        "http://localhost:8082/*"
      ],
      "webOrigins": [
        "https://adminer.test",
        "http://localhost:8082"
      ],
      "protocol": "openid-connect",
      "publicClient": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true
    },
    {
      "clientId": "grafana",
      "name": "Grafana Monitoring",
      "description": "Metrics and monitoring dashboards",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "grafana-secret",
      "redirectUris": [
        "https://grafana.test/*",
        "http://localhost:9001/*"
      ],
      "webOrigins": [
        "https://grafana.test",
        "http://localhost:9001"
      ],
      "protocol": "openid-connect",
      "publicClient": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true
    },
    {
      "clientId": "metabase",
      "name": "Metabase Analytics",
      "description": "Business intelligence and analytics",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "metabase-secret",
      "redirectUris": [
        "https://metabase.test/*",
        "http://localhost:8085/*"
      ],
      "webOrigins": [
        "https://metabase.test",
        "http://localhost:8085"
      ],
      "protocol": "openid-connect",
      "publicClient": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true
    },
    {
      "clientId": "microservices-api",
      "name": "Microservices API Gateway",
      "description": "Main API gateway for microservices",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "api-gateway-secret",
      "redirectUris": ["*"],
      "webOrigins": ["*"],
      "protocol": "openid-connect",
      "publicClient": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "admin",
        "description": "Administrator role with full access",
        "composite": false
      },
      {
        "name": "developer",
        "description": "Developer role with development access",
        "composite": false
      },
      {
        "name": "user",
        "description": "Standard user role",
        "composite": false
      },
      {
        "name": "viewer",
        "description": "Read-only access role",
        "composite": false
      }
    ]
  },
  "groups": [
    {
      "name": "Administrators",
      "path": "/Administrators",
      "realmRoles": ["admin"],
      "subGroups": []
    },
    {
      "name": "Developers",
      "path": "/Developers", 
      "realmRoles": ["developer"],
      "subGroups": []
    },
    {
      "name": "Users",
      "path": "/Users",
      "realmRoles": ["user"],
      "subGroups": []
    }
  ],
  "users": [
    {
      "username": "admin",
      "email": "admin@site.test",
      "firstName": "System",
      "lastName": "Administrator",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "123456",
          "temporary": false
        }
      ],
      "realmRoles": ["admin", "user"],
      "groups": ["/Administrators"]
    },
    {
      "username": "developer",
      "email": "dev@site.test",
      "firstName": "System",
      "lastName": "Developer",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "123456",
          "temporary": false
        }
      ],
      "realmRoles": ["developer", "user"],
      "groups": ["/Developers"]
    },
    {
      "username": "testuser",
      "email": "test@site.test",
      "firstName": "Test",
      "lastName": "User",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "123456",
          "temporary": false
        }
      ],
      "realmRoles": ["user"],
      "groups": ["/Users"]
    }
  ],
  "identityProviders": [],
  "identityProviderMappers": [],
  "internationalizationEnabled": true,
  "supportedLocales": ["en", "de", "fr", "es", "it", "ja", "pt-BR", "ru", "zh-CN"],
  "defaultLocale": "en",
  "loginTheme": "microservices",
  "accountTheme": "microservices",
  "adminTheme": "keycloak",
  "emailTheme": "microservices",
  "eventsEnabled": true,
  "eventsExpiration": 1209600,
  "eventsListeners": ["jboss-logging"],
  "enabledEventTypes": [
    "LOGIN",
    "LOGOUT",
    "REGISTER",
    "UPDATE_PASSWORD",
    "UPDATE_EMAIL",
    "UPDATE_PROFILE"
  ],
  "adminEventsEnabled": true,
  "adminEventsDetailsEnabled": true,
  "attributes": {
    "frontendUrl": "https://keycloak.test",
    "_browser_header.contentSecurityPolicyReportOnly": "",
    "_browser_header.xContentTypeOptions": "nosniff",
    "_browser_header.xRobotsTag": "none",
    "_browser_header.xFrameOptions": "SAMEORIGIN",
    "_browser_header.contentSecurityPolicy": "frame-src 'self'; frame-ancestors 'self'; object-src 'none';"
  }
}